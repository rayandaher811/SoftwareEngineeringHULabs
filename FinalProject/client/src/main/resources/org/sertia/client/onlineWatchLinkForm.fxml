                    <font>
                        <Font size="26.0"/>
                    </font>
                </Label>
            </top>
            <center>
                <VBox alignment="CENTER">
                    <children>
                        <ListView fx:id="lstView" prefHeight="200.0" prefWidth="300.0" BorderPane.alignment="CENTER"/>
                    </children>
                    <children>
                        <Button fx:id="backBtn" onAction="#toMain" text="חזור"/>
