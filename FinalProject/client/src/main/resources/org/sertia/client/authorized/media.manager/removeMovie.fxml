<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.sertia.client.views.authorized.media.manager.RemoveMovie"
            prefHeight="400.0" prefWidth="600.0">
    <children>
    <BorderPane prefHeight="368.0" prefWidth="618.0">
        <top>
            <Label alignment="TOP_CENTER" prefHeight="38.0" prefWidth="331.0" text="מחיקת סרט מהסרטיה"
                   BorderPane.alignment="CENTER">
                <font>
                    <Font size="26.0"/>
                </font>
            </Label>
        </top>
        <center>
            <VBox alignment="CENTER">
                <children>
                    <ComboBox fx:id="movieToRemove" maxWidth="500" maxHeight="300">
                    </ComboBox>
                </children>
                <children>
                    <Button onMouseClicked="#requestRemove">שליחת תלונה</Button>
                </children>
            </VBox>
        </center>
    </BorderPane>
    </children>
    <Button fx:id="backBtn" layoutX="293.0" layoutY="365.0" mnemonicParsing="false" onAction="#backToMenu" text="חזרה"/>
</AnchorPane>
